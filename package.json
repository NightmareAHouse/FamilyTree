{
  "name": "family_tree",
  "version": "0.1.0",
  "private": true,
  "main": "electron/main.js",
  "dependencies": {
    "concurrently": "^9.0.0",
    "web-vitals": "^2.1.4"
  },
  "scripts": {
    "start-react": "cd frontend && npm run start",
    "build-react": "cd frontend && npm run build",
